DROP KEYSPACE MATCH;
DROP KEYSPACE PLAYER;
DROP KEYSPACE TEAM;

CREATE KEYSPACE MATCH
WITH replication = { 'class' : 'SimpleStrategy', 'replication_factor' : 3};

CREATE KEYSPACE PLAYER
WITH replication = { 'class' : 'SimpleStrategy', 'replication_factor' : 3};

CREATE KEYSPACE TEAM
WITH replication = { 'class' : 'SimpleStrategy', 'replication_factor' : 3};

CREATE TABLE MATCH.MATCH_BY_ID (
MATCH_DATE DATE,
MATCH_ID SMALLINT,
HOST_ID SMALLINT,
GUEST_ID SMALLINT,
SCORE TEXT,
HOST_NAME TEXT,
GUEST_NAME TEXT,
PRIMARY KEY(MATCH_ID, MATCH_DATE));

CREATE TABLE MATCH.MATCH_BY_DATE (
MATCH_DATE DATE,
MATCH_ID SMALLINT,
HOST_ID SMALLINT,
GUEST_ID SMALLINT,
SCORE TEXT,
HOST_NAME TEXT,
GUEST_NAME TEXT,
PRIMARY KEY(MATCH_DATE, MATCH_ID))
WITH CLUSTERING ORDER BY (MATCH_ID ASC);

CREATE TABLE PLAYER.PLAYER_BY_ID (
PLAYER_ID SMALLINT,
TEAM_ID SMALLINT,
TEAM_NAME TEXT,
PLAYER_NAME TEXT,
NUMBER SMALLINT,
AGE SMALLINT,
HEIGHT SMALLINT,
PRIMARY KEY(PLAYER_ID, PLAYER_NAME))
WITH CLUSTERING ORDER BY (PLAYER_NAME ASC);

CREATE TABLE TEAM.TEAM_BY_ID (
TEAM_ID SMALLINT,
TEAM_NAME TEXT,
CITY TEXT,
PRIMARY KEY(TEAM_ID, TEAM_NAME))
WITH CLUSTERING ORDER BY (TEAM_NAME ASC);

CREATE TABLE TEAM.TEAM_BY_CITY (
TEAM_ID SMALLINT,
TEAM_NAME TEXT,
CITY TEXT,
PRIMARY KEY(CITY, TEAM_NAME))
WITH CLUSTERING ORDER BY (TEAM_NAME ASC);

CREATE TABLE MATCH.GOAL_BY_MATCH (
MATCH_DATE DATE,
MATCH_ID SMALLINT,
PLAYER_ID SMALLINT,
TEAM_ID SMALLINT,
MINUTE SMALLINT,
PLAYER_NAME TEXT,
TEAM_NAME TEXT,
PRIMARY KEY(MATCH_ID, MINUTE))
WITH CLUSTERING ORDER BY (MINUTE ASC);

CREATE TABLE MATCH.GOAL_BY_PLAYER (
MATCH_DATE DATE,
MATCH_ID SMALLINT,
PLAYER_ID SMALLINT,
TEAM_ID SMALLINT,
MINUTE SMALLINT,
PLAYER_NAME TEXT,
TEAM_NAME TEXT,
PRIMARY KEY(PLAYER_NAME, MATCH_ID, MINUTE));


CREATE TABLE PLAYER.PLAYER_BY_TEAM (
PLAYER_ID SMALLINT,
TEAM_ID SMALLINT,
TEAM_NAME TEXT,
PLAYER_NAME TEXT,
NUMBER SMALLINT,
AGE SMALLINT,
HEIGHT SMALLINT,
PRIMARY KEY(TEAM_NAME, NUMBER))
WITH CLUSTERING ORDER BY (NUMBER ASC);


