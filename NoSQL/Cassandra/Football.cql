USE MATCH;
// 1. Матч по ID 
SELECT MATCH_DATE, HOST_NAME, GUEST_NAME, SCORE
FROM MATCH_BY_ID
WHERE MATCH_ID = 1 and MATCH_DATE = '2019-11-25';


SELECT MATCH_DATE, HOST_NAME, GUEST_NAME, SCORE
FROM MATCH_BY_ID
WHERE MATCH_DATE = '2019-11-23';

// 2. Матчи за 23 ноября
SELECT HOST_NAME, GUEST_NAME, SCORE
FROM MATCH_BY_DATE
WHERE MATCH_DATE = '2019-11-23';

// 3. Все голы в матче
SELECT TEAM_NAME, PLAYER_NAME, MINUTE
FROM GOAL_BY_MATCH
WHERE MATCH_ID = 2
ORDER BY MINUTE;

// 4. Количество забитых голов каждым игроком
SELECT PLAYER_NAME, COUNT(*) AS TOTAL
FROM GOAL_BY_PLAYER
GROUP BY PLAYER_NAME;//, MATCH_ID;

// 5. Все голы футболиста Alexandre Lacazette
SELECT MATCH_DATE, TEAM_NAME, MINUTE
FROM GOAL_BY_PLAYER
WHERE PLAYER_NAME = 'Alexandre Lacazette';

USE PLAYER;
// 6. Игрок по ID 
SELECT TEAM_NAME, PLAYER_NAME, NUMBER, HEIGHT, AGE
FROM PLAYER_BY_ID
WHERE PLAYER_ID = 5;

// 7. Все футболисты Sheffield United
SELECT NUMBER, PLAYER_NAME, AGE, HEIGHT
FROM PLAYER_BY_TEAM
WHERE TEAM_NAME = 'Sheffield United'
ORDER BY NUMBER;

// 8. Количество игроков в клубах
SELECT TEAM_NAME, COUNT(NUMBER) AS TOTAL
FROM PLAYER_BY_TEAM
GROUP BY TEAM_NAME;

USE TEAM;
// 9. Команда по ID 
SELECT TEAM_NAME, CITY
FROM TEAM_BY_ID
WHERE TEAM_ID = 1;

// 10. Все клубы Лондона
SELECT TEAM_NAME
FROM TEAM_BY_CITY
WHERE CITY = 'London';